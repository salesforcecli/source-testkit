{
  "name": "@salesforce/source-testkit",
  "description": "testkit for running NUTs for source related commands",
  "version": "1.2.15",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "salesforcecli/source-testkit",
  "scripts": {
    "build": "sf-build && yarn copy-metadata",
    "ci-docs": "yarn sf-ci-docs",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "compile": "sf-compile",
    "copy-metadata": "copyfiles -u 1 'src/metadata/**/*' 'lib/metadata'",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "lint-fix": "yarn sf-lint --fix",
    "prepack": "sf-prepack && yarn copy-metadata",
    "prepare": "sf-install",
    "pretest": "sf-compile-test",
    "test": "sf-test"
  },
  "files": [
    "docs",
    "lib",
    "messages",
    "!lib/**/*.map"
  ],
  "dependencies": {
    "@salesforce/cli-plugins-testkit": "^2.5.2",
    "@salesforce/core": "^3.30.2",
    "@salesforce/kit": "^1.5.42",
    "@salesforce/source-deploy-retrieve": "^6.8.1",
    "@salesforce/ts-types": "^1.5.21",
    "archiver": "^5.2.0",
    "chai-each": "^0.0.1",
    "debug": "^4.3.1",
    "shelljs": "^0.8.4",
    "sinon": "^10.0.0",
    "strip-ansi": "^7.0.1"
  },
  "devDependencies": {
    "@salesforce/dev-config": "^3.0.0",
    "@salesforce/dev-scripts": "^2.0.2",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.4.0",
    "@types/archiver": "^5.1.0",
    "@types/debug": "^4.1.5",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "chai": "^4.3.0",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.8",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "^35.1.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "sinon": "^10.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.8.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}